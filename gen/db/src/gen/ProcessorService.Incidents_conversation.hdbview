VIEW ProcessorService_Incidents_conversation AS SELECT
  conversation_0.up__ID,
  conversation_0.ID,
  conversation_0."TIMESTAMP",
  conversation_0.author,
  conversation_0.message
FROM sap_capire_incidents_Incidents_conversation AS conversation_0
WITH ASSOCIATIONS (
  MANY TO ONE JOIN ProcessorService_Incidents AS up_ ON (up_.ID = up__ID)
)